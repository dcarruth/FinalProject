<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carruth.finalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dr_vinyl"
        android:label=""
        android:roundIcon="@mipmap/dr_vinyl"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateAccountActivity"
            android:label="@string/create_account_button"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".CreateNewPasswordActivity"
            android:label="@string/forget_password"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/login"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ServiceActivity"
            android:screenOrientation="portrait"
            android:label="Services" />
        <activity
            android:name=".CalanderActivity"
            android:screenOrientation="portrait"
            android:label="Choose a Date" />
        <activity android:name=".ChooseTimeActivity"
            android:screenOrientation="portrait"
            android:label="Choose a Time"/>
    </application>

</manifest>